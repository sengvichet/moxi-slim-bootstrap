  <h6>Website Emailing Information</h6>
  <!-- Email Host -->
  <div class="form-group row">
    <label for="email-host-select" class="col-sm-3 col-form-label">Email Host</label>
    <div class="col-sm-9">
      <select id="email-host-select" name="email_host"
        class="form-control {% if messages.error.email_information.email_host is defined %}is-invalid{% endif %}">
          <!-- option Outlook -->
          <option value="outlook"
          {% if messages.input.email_host is defined
            and messages.input.email_host == 'outlook' %}selected{% else %}
          {% if website_information.data.email_information.host is defined
            and website_information.data.email_information.host == 'outlook' %}selected{% endif %}{% endif %}>Outlook 365</option>
          <!-- option Gmail -->
          <option value="gmail"
          {% if messages.input.email_host is defined
            and messages.input.email_host == 'gmail' %}selected{% else %}
          {% if website_information.data.email_information.host is defined
            and website_information.data.email_information.host == 'gmail' %}selected{% endif %}{% endif %}>Gmail</option>
          <!-- option other -->
          <option value="other"
          {% if messages.input.email_host is defined
            and messages.input.email_host == 'other' %}selected{% else %}
          {% if website_information.data.email_information.host is defined
            and website_information.data.email_information.host == 'other' %}selected{% endif %}{% endif %}>Other</option>
        </select>
      <div class="valid-feedback">Looks good!</div>
      {% if messages.error.email_information.email_host is defined %}
        {% for error in messages.error.email_information.email_host %}
          <div class="invalid-feedback">{{ error }}</div>
        {% endfor %}
      {% else %}
        <div class="invalid-feedback">Please provide a valid email host.</div>
      {% endif %}
    </div>
  </div>
