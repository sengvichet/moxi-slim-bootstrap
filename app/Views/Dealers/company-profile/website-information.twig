<h5 class="text-uppercase">Website Information</h5>
<div class="alert alert-info font-weight-bold">Please fill out as much of this information as possible. If you are unable to provide all of the information requested on this page, please be sure to include contact info for your IT service. We will reach out to them for any information you are unable to provide.</div>
{% include 'Dealers/partials/errors.twig' %}
<form action="/{{ website_information.action }}" method="POST" id="website-information-form" class="needs-validation" novalidate>
  <input type="hidden" name="company_id" value="{% if contact_information.data.company %}{{ contact_information.data.company.id }}{% endif %}">
  <h6>Basic Website Information</h6>
  <!-- Website URL -->
  <div class="form-group row">
    <label for="website-url-input" class="col-sm-3 col-form-label">Website URL</label>
    <div class="col-sm-9">
      <input type="url" id="website-url-input"
        class="form-control {% if messages.error.website_information.website_url is defined %}is-invalid{% endif %}"
        name="website_url" placeholder="http://website.net" required
        maxlength="{{ fields.company.website.max_length }}"
        value="{% if messages.input.website_url is defined and messages.error.count is defined and messages.error.count %}{{ messages.input.website_url }}{% else %}{% if contact_information.data.company.website is defined %}{{ contact_information.data.company.website }}{% endif %}{% endif %}">
      <div class="valid-feedback">Looks good!</div>
      {% if messages.error.website_information.website_url is defined %}
        {% for error in messages.error.website_information.website_url %}
          <div class="invalid-feedback">{{ error }}</div>
        {% endfor %}
      {% else %}
        <div class="invalid-feedback">Hint: try pasting your website link directly from the web browser.</div>
      {% endif %}
    </div>
  </div>
  <!-- Website Name -->
  <div class="form-group row">
    <label for="website-name-input" class="col-sm-3 col-form-label">Name of Website</label>
    <div class="col-sm-9">
      <input type="text" id="website-name-input"
        class="form-control {% if messages.error.website_information.website_name is defined %}is-invalid{% endif %}"
        name="website_name" placeholder="Name of Website"
        maxlength="{{ fields.website_information.website_name.max_length }}"
        value="{% if messages.input.website_name is defined and messages.error.count is defined and messages.error.count  %}{{ messages.input.website_name }}{% else %}{% if website_information.data.website_information.name is defined %}{{ website_information.data.website_information.name }}{% endif %}{% endif %}">
      <div class="valid-feedback">Looks good!</div>
      {% if messages.error.website_information.website_name is defined %}
        {% for error in messages.error.website_information.website_name %}
          <div class="invalid-feedback">{{ error }}</div>
        {% endfor %}
      {% else %}
        <div class="invalid-feedback">Please provide a valid website name.</div>
      {% endif %}
    </div>
  </div>
  <!-- Website Phone -->
  <div class="form-group row">
    <label for="website-phone-input" class="col-sm-3 col-form-label">Company Phone</label>
    <div class="col-sm-9">
